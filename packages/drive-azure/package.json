{
	"name": "@directus/drive-azure",
	"version": "9.19.5",
	"description": "Azure Blob driver for @directus/drive",
	"license": "MIT",
	"main": "dist/index.js",
	"exports": {
		".": "./dist/index.js",
		"./package.json": "./package.json"
	},
	"types": "dist/index.d.ts",
